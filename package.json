    "theme",
    "colour-theme",
    "dark",
    "light",
    "soft grey"
  ],
  "icon": "soft-grey-icon.png",
  "contributes": {
    "themes": [
      {
        "label": "Soft Grey Dark",
        "uiTheme": "vs-dark",
